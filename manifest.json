{
    "manifest_version": 3,
    "name": "Context Menu Test",
    "version": "1.0",
    "description": "A simple context menu extension",
    "permissions": [
        "contextMenus",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}